#!/usr/bin/python
# -*- coding:UTF-8 -*-)
a = [int(x) for x in input("Unesi brojeve: ").split(" ")]
a.sort()
print("najmanji: ", end="")
print(a[0])
print("najveÄ‡i: ", end="")
i = len(a)-1
print(a[i])

#3
#!/usr/bin/python
# -*- coding:UTF-8 -*-)
lista = [int(x) for x in input("Unesi brojeve: ").split(" ")]
a = []
b = []
index_a = 0
index_b = 0
i=0
for i in range(len(lista)):
	if lista[i] % 2:
		a.insert(index_a, lista[i])
		index_a = index_a + 1
	else:
		b.insert(index_b, lista[i])
		index_b = index_b + 1
	i = i+1
print("neparni: ")
print(a)
print("parni: ")
print(b)

#4
#!/usr/bin/python
# -*- coding:UTF-8 -*-
x = int(input("Unesi broj: "))
def prost_broj(x):
	i = 2
	while i <= x:
		if x % i != 0:
			i = i+1
		if x % i == 0 and i < x:
			return False
		if x % i == 0 and i == x:
			print(x)
			return True
output = prost_broj(x)
print(output)
