#!/usr/bin/python
# -*- coding:UTF-8 -*-
a = 1
exp = 0
while 1:
	a = 2 ** exp
	exp = exp + 1
	if a<1000:
		print(a)

#2
#!/usr/bin/python
# -*- coding:UTF-8 -*-)
a = [int(x) for x in input("Unesi brojeve: ").split(" ")]
a.sort()
print("najmanji: ", end="")
print(a[0])
print("najveÄ‡i: ", end="")
i = len(a)-1
print(a[i])

#3
#!/usr/bin/python
# -*- coding:UTF-8 -*-)
lista = [int(x) for x in input("Unesi brojeve: ").split(" ")]
a = []
b = []
index_a = 0
index_b = 0
i=0
for i in range(len(lista)):
	if lista[i] % 2:
		a.insert(index_a, lista[i])
		index_a = index_a + 1
	else:
		b.insert(index_b, lista[i])
		index_b = index_b + 1
	i = i+1
print("neparni: ")
print(a)
print("parni: ")
print(b)
